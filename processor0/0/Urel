FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      Urel;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    right
    {
        type            cyclic;
    }
    left
    {
        type            cyclic;
    }
    outerWall
    {
        type            rotatingWallVelocity;
        origin          (0 0 0);
        axis            (0 0 1);
        omega           
        {
            type            table;
            values          
3
(
(0 -5.23598775)
(2500 -7.853981625)
(5000 -20.943951)
)
;
        }
        value           nonuniform List<vector> 0();
    }
    stirrerWall
    {
        type            noSlip;
    }
    top
    {
        type            empty;
    }
    bottom
    {
        type            empty;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary0to3
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


