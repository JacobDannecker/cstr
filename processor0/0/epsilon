FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.1;

boundaryField
{
    right
    {
        type            cyclic;
    }
    left
    {
        type            cyclic;
    }
    outerWall
    {
        type            epsilonWallFunction;
        value           nonuniform List<scalar> 0();
    }
    stirrerWall
    {
        type            epsilonWallFunction;
        value           uniform 0.0001;
    }
    top
    {
        type            empty;
    }
    bottom
    {
        type            empty;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 0.1;
    }
    procBoundary0to3
    {
        type            processor;
        value           uniform 0.1;
    }
}


// ************************************************************************* //
