FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      Urel;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    top
    {
        type            empty;
        
    }
    bottom
    {
        type		empty;
    }
    outerWall
    {
        type            rotatingWallVelocity;
        origin          (0 0 0);
        axis            (0 0 1);
        omega           table 
	((0	-5.23598775)
	 (2500	-7.853981625) 
	 (5000 -20.943951)
	);
    }
    stirrerWall
    {
        type            noSlip;
    }
    
    right
    {
        type            cyclic;
    }
    left
    {
        type            cyclic;
    }
}


