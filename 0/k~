FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.0001;

boundaryField
{
    top
    {
        type            empty;
    }
    bottom
    {
        type            empty;

    }
    stirrerWall
    {
        type            kqRWallFunction;
        value           uniform 0.0001;
    }
    outerWall
    {
        type            kqRWallFunction;
        value           uniform 0.0001;
    }
    right
    {
        type            cyclic;
        value           uniform 0.0001;
    }
    left
    {
        type            cyclic;
        value           uniform 0.0001;
    }
}


